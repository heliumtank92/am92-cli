{
  "name": "@am92/cli",
  "version": "0.0.45",
  "description": "Command line interface of utilities for @am92 packages and boilerplates",
  "sideEffects": false,
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "main": "dist/index.js",
  "bin": {
    "am92": "./dist/index.js"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/**/*.json"
  ],
  "dependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-typescript": "^7.23.3",
    "colorette": "^2.0.20",
    "prompt-sync": "^4.2.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-private-methods": "^7.23.3",
    "@types/babel__core": "^7.20.5",
    "@types/node": "^20.3.1",
    "@types/prompt-sync": "^4.2.3",
    "@types/yargs": "^17.0.32",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.1.3"
  },
  "scripts": {
    "temp-test": "npm run build; npm uninstall -g @am92/cli; npm i -g .",
    "compile": "tsc",
    "build:clean": "rm -rf ./dist",
    "build": "npm-run-all build:clean compile",
    "copy-files": "copyfiles -u 1 src/**/*.json ./dist/",
    "test": "echo 'Error: no test specified'",
    "preversion": "npm-run-all build test",
    "postversion": "git push && git push --tags && npm run publish:pkg",
    "publish:pkg": "npm publish"
  },
  "repository": "git@github.com:heliumtank92/am92-cli.git",
  "homepage": "https://heliumtank92.github.io/am92-cli",
  "contributors": [
    {
      "name": "Ankit Gandhi",
      "email": "ankitgandhi452@gmail.com",
      "url": "https://github.com/ankitgandhi452"
    },
    {
      "name": "Mehul Agarwal",
      "email": "agarwalmehul92@gmail.com",
      "url": "https://github.com/agarwalmehul"
    }
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "am92",
    "cli",
    "am92 cli"
  ]
}
